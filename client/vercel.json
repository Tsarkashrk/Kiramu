{
  "routes": [
    {
      "src": "/auth/register",
      "dest": "https://kiramu-anime.vercel.app/auth/register"
    },
    {
      "src": "/auth/login",
      "dest": "https://kiramu-anime.vercel.app/auth/login"
    },
    {
      "src": "/anime/:animeCode/",
      "dest": "https://kiramu-anime.vercel.app/anime/:animeCode/"
    },
    {
      "src": "/profile",
      "dest": "https://kiramu-anime.vercel.app/profile"
    },
    {
      "src": "/profile/favorites",
      "dest": "https://kiramu-anime.vercel.app/profile/favorites"
    }
  ]
}
